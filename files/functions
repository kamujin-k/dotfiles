porta() {
    filter="sport = :$1"

    sudo ss -lptn $filter
}


reload() {
    tmux source-file ~/.tmux.conf
    if [[ $SHELL == *"zsh"* ]]; then
        source ~/.zshrc
    else
	source ~/.bashrc
    fi
}


b64(){
   echo -n $1 | base64 $2
}


sdkmenu() {
    sdk use java  $(sdk list java | grep "installed\|local only" | awk -F\| '{print $6}' | fzf)
}

